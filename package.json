{
  "name": "fantascribe",
  "version": "0.0.1",
  "private": true,
  "description": "UI for map & language generators",
  "main": "src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --template src/template.html",
    "serve": "preact build --no-prerender --template src/template.html && preact serve",
    "dev": "preact watch --template src/template.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.6",
    "eslint": "^6.0.1",
    "if-env": "^1.0.4",
    "node-sass": "^4.12.0",
    "preact-cli": "^2.2.1",
    "preact-redux": "^2.1.0",
    "preact-render-spy": "^1.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "linkref": "^1.1.0",
    "preact": "^8.4.2",
    "preact-router": "^2.6.1"
  }
}
